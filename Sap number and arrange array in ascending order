arr = [5,4,6,1,2,5]
n = len(arr)
    
result = sorted(arr)
if result == arr:
    print (1)
    
for lst in range(1,n-1):   
    first = arr.pop(0)
    arr.append(first)
    print(arr)
    
    i = n-1
    while i > 0 and arr[i] < arr[i-1]:
        arr[i], arr[i-1] = arr[i-1], arr[i]
        i -= 1
    
    if list(arr) == result:
        print(lst)
